{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "allowJs": true,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@shared/shared": [
        "libs/shared/src"
      ],
      "@shared/shared/*": [
        "libs/shared/src/*"
      ],
      "@highest-open-value/highest-open-value": [
        "libs/highest-open-value/src"
      ],
      "@highest-open-value/highest-open-value/*": [
        "libs/highest-open-value/src/*"
      ]
    }
  }
}